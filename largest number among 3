#include<stdio.h>
int main()
{
  
   int a ,b,c;
   printf("Enter the three numbers to be check the greater one");
   scanf("%d%d%d",&a,&b,&c);
   if (a>b  &&  a>>c)
 {
    printf(" a is the largest number);
    }
    else if (b>a && b>c)
    {
      print("b is the greater number");
      }
      else if (c>a && c>b)
      {
       printf("c is the greater number");
       }
       else
       {
       printf("invalid input")
       }
       
       return 0;
       }
